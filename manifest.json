{
    "manifest_version": 4,
    "type": "extension",
    "name": "Animate动画插件",
    "slug": "animate",
    "description": "LiteLoaderQQNT的Animate动画插件",
    "version": "1.0.0",
    "icon": "./icon.png",
    "authors": [
        {
            "name": "Adoin",
            "link": "https://github.com/adoin"
        }
